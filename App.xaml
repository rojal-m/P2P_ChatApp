             xmlns:local="clr-namespace:P2P_Chat_App"
             Startup="Main">
    <Application.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary Source=" ./Themes/ContactCard.xaml"/>
